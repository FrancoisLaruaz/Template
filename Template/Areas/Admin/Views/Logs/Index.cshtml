@model Models.ViewModels.LogsViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.Partial("_PageTitle")


<div class="main-content">
    <div class="col-md-12 no-gutter">
        <div style="margin-top:20px;margin-bottom:20px" id="infosconnectionBDD">
            <span style="font-weight:bold;font-size:16px;margin-right:15px;">Informations connection BDD :</span>
            @Html.Raw(@Model.InfoConnectionBBD)
        </div>
        <div class="row">
            <div class="col-xs-12 col-md-4 col-md-offset-4">
                <input id="SearchBox" type="text" class="form-control searchTexbox" />
                <a style="margin-left:5px;" role="button" aria-haspopup="true" aria-expanded="false" title="Refresh logs" id="SearchIcon" onclick="RefreshData(true)"><span class="glyphicon glyphicon-search"></span></a>
            </div>
        </div>
        <div id="targetContainer" style="padding-top:20px">

        </div>

    </div>
</div>




@section scripts {
    @Html.ScriptImportContent("~/Areas/Admin/Scripts/Views/Logs/Index.js")
}

