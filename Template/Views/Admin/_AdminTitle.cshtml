@model Models.Class.IAdminViewModel


<div style="text-align:center;margin-top:0px">
    <h1 style="color:black;font-size:50px">@Model.Title</h1>
</div>
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div style="text-align:center" class="col-xs-4 col-xs-offset-4 col-md-2 col-md-offset-5 row" align="center">
            <div style="text-align:center;border-top:solid 5px #6699ff;height:25px;" class="col-xs-6 col-xs-offset-3" align="center"></div>
        </div>
    </div>
    @if (!string.IsNullOrEmpty(Model.Description))
    {
    <div class="col-xs-12 col-md-12">
        <div style="font-size:15px;padding-bottom:35px" class="col-xs-12 col-md-8 col-md-offset-2">
            @Model.Description
        </div>
    </div>
    }
</div>